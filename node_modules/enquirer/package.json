{
  "name": "enquirer",
  "description": "Intuitive, plugin-based prompt system for node.js.",
  "version": "1.0.3",
  "homepage": "http://enquirer.io",
  "author": "Jon Schlinkert (https://github.com/jonschlinkert)",
  "contributors": [
    "Brian Woodward (https://twitter.com/doowb)",
    "Jon Schlinkert (http://twitter.com/jonschlinkert)",
    "Michael KÃ¼hnel (http://michael-kuehnel.de)"
  ],
  "repository": "enquirer/enquirer",
  "bugs": {
    "url": "https://github.com/enquirer/enquirer/issues"
  },
  "license": "MIT",
  "files": [
    "index.js"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=5.0"
  },
  "scripts": {
    "test": "mocha"
  },
  "dependencies": {
    "choices-separator": "^2.0.0",
    "collection-visit": "^1.0.0",
    "component-emitter": "^1.2.1",
    "debug": "^2.6.8",
    "extend-shallow": "^2.0.1",
    "get-value": "^2.0.6",
    "isobject": "^3.0.0",
    "promise-reduce": "^2.1.0",
    "prompt-question": "^3.0.3",
    "prompt-input": "^3.0.0",
    "readline-ui": "^2.2.2",
    "set-value": "^1.0.0"
  },
  "devDependencies": {
    "gulp": "^3.9.1",
    "gulp-eslint": "^3.0.1",
    "gulp-format-md": "^0.1.12",
    "gulp-istanbul": "^1.1.1",
    "gulp-mocha": "^3.0.1",
    "gulp-unused": "^0.2.1",
    "mocha": "^3.4.2",
    "strip-color": "^0.1.0"
  },
  "keywords": [
    "answer",
    "answers",
    "ask",
    "checkbox",
    "choice",
    "cli",
    "command",
    "enquirer",
    "input",
    "inquire",
    "inquirer",
    "interact",
    "list",
    "menu",
    "password",
    "prompt",
    "prompts",
    "question",
    "readline",
    "stdin",
    "stdout",
    "terminal",
    "tty",
    "ui"
  ],
  "lintDeps": {
    "devDependencies": {
      "files": {
        "options": {
          "ignore": [
            "examples/**",
            "support/**"
          ]
        }
      }
    }
  },
  "verb": {
    "toc": true,
    "layout": "common-minimal",
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "lint": {
      "reflinks": true
    },
    "related": {
      "prompts": [
        "prompt-autocompletion",
        "prompt-checkbox",
        "prompt-confirm",
        "prompt-editor",
        "prompt-expand",
        "prompt-input",
        "prompt-list",
        "prompt-password",
        "prompt-question",
        "prompt-radio",
        "prompt-rawlist"
      ]
    },
    "reflinks": [
      "enquirer",
      "enquirer-prompts",
      "enquirer-question",
      "prompt-base"
    ]
  }
}
