{
  "name": "prompt-rawlist",
  "description": "Rawlist prompt. Can be used as a standalone prompt, or with a prompt system like [Enquirer].",
  "version": "2.0.1",
  "homepage": "https://github.com/enquirer/prompt-rawlist",
  "author": "Jon Schlinkert (https://github.com/jonschlinkert)",
  "contributors": [
    "Brian Woodward (https://twitter.com/doowb)",
    "Jon Schlinkert (http://twitter.com/jonschlinkert)"
  ],
  "repository": "enquirer/prompt-rawlist",
  "bugs": {
    "url": "https://github.com/enquirer/prompt-rawlist/issues"
  },
  "license": "MIT",
  "files": [
    "index.js"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=5.0"
  },
  "scripts": {
    "test": "mocha"
  },
  "dependencies": {
    "ansi-cyan": "^0.1.1",
    "ansi-dim": "^0.1.1",
    "ansi-red": "^0.1.1",
    "is-number": "^3.0.0",
    "prompt-list": "^2.0.1"
  },
  "devDependencies": {
    "enquirer": "^1.0.2",
    "gulp-format-md": "^0.1.12",
    "mocha": "^3.4.2"
  },
  "keywords": [
    "answer",
    "answers",
    "ask",
    "checkbox",
    "choice",
    "cli",
    "command",
    "enquirer",
    "enquirerprompt",
    "input",
    "inquire",
    "inquirer",
    "interact",
    "list",
    "menu",
    "password",
    "prompt",
    "prompts",
    "question",
    "rawlist",
    "readline",
    "stdin",
    "stdout",
    "terminal",
    "tty",
    "ui"
  ],
  "verb": {
    "toc": false,
    "layout": "common-minimal",
    "data": {
      "alias": "rawlist"
    },
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "related": {
      "list": [
        "prompt-base",
        "prompt-checkbox",
        "prompt-confirm",
        "prompt-list",
        "prompt-question",
        "prompt-radio"
      ]
    },
    "lint": {
      "reflinks": true
    },
    "reflinks": [
      "enquirer",
      "inquirer"
    ]
  }
}
