{
  "name": "prompt-list",
  "description": "List-style prompt. Can be used as a standalone prompt, or with a prompt system like [enquirer].",
  "version": "2.2.0",
  "homepage": "https://github.com/enquirer/prompt-list",
  "author": "Jon Schlinkert (https://github.com/jonschlinkert)",
  "contributors": [
    "Brian Woodward (https://twitter.com/doowb)",
    "Jon Schlinkert (http://twitter.com/jonschlinkert)"
  ],
  "repository": "enquirer/prompt-list",
  "bugs": {
    "url": "https://github.com/enquirer/prompt-list/issues"
  },
  "license": "MIT",
  "files": [
    "index.js"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=5.0"
  },
  "scripts": {
    "test": "mocha"
  },
  "dependencies": {
    "ansi-cyan": "^0.1.1",
    "ansi-dim": "^0.1.1",
    "debug": "^3.0.1",
    "prompt-radio": "^1.2.1"
  },
  "devDependencies": {
    "choices-separator": "^2.0.0",
    "enquirer": "^1.0.2",
    "gulp-format-md": "^1.0.0",
    "mocha": "^3.5.0"
  },
  "keywords": [
    "answer",
    "answers",
    "ask",
    "checkbox",
    "choice",
    "cli",
    "command",
    "enquirer-prompt",
    "enquirerprompt",
    "input",
    "inquire",
    "inquirer",
    "interact",
    "list",
    "menu",
    "password",
    "plugin",
    "prompt",
    "prompts",
    "question",
    "readline",
    "stdin",
    "stdout",
    "terminal",
    "tty",
    "ui"
  ],
  "verb": {
    "toc": false,
    "layout": "default",
    "tasks": [
      "readme"
    ],
    "data": {
      "alias": "list"
    },
    "plugins": [
      "gulp-format-md"
    ],
    "related": {
      "list": [
        "enquirer",
        "enquirer-prompt",
        "enquirer-question"
      ]
    },
    "lint": {
      "reflinks": true
    }
  }
}
