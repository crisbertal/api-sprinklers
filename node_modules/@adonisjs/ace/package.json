    "cz-conventional-changelog": "^3.3.0",
    "japa": "^3.1.1",
    "mrm": "^2.6.1",
    "pkg-ok": "^2.3.1",
    "semver": "^7.3.5",
    "standard": "^16.0.3"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "dependencies": {
    "cli-table": "^0.3.1",
    "commander": "^2.18.0",
    "debug": "^4.3.1",
    "enquirer": "^1.0.3",
    "fast-levenshtein": "^3.0.0",
    "fs-extra": "^9.1.0",
    "is-arrow-function": "^2.0.3",
    "kleur": "^2.0.2",
