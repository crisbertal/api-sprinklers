{
  "name": "@adonisjs/bodyparser",
  "version": "2.3.0",
  "description": "Body parser middleware for Adonis 4.0 and above",
  "main": "index.js",
  "files": [
    "config",
    "providers",
    "src",
    "instructions.js",
    "instructions.md"
  ],
  "scripts": {
    "mrm": "mrm --preset=@adonisjs/mrm-preset",
    "pretest": "npm run lint",
    "test": "node japaFile.js",
    "test:win": "node japaFile.js",
    "commit": "git-cz",
    "lint": "standard"
  },
  "standard": {
    "global": [
      "use",
      "it",
      "describe",
      "context",
      "before",
      "after"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adonisjs/adonis-bodyparser.git"
  },
  "keywords": [
    "adonis",
    "http-middleware",
    "middleware",
    "adonisjs"
  ],
  "author": "adonisjs",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/adonisjs/adonis-bodyparser/issues"
  },
  "homepage": "https://github.com/adonisjs/adonis-bodyparser#readme",
  "dependencies": {
    "@adonisjs/generic-exceptions": "^2.0.1",
    "bytes": "^3.0.0",
    "co-body": "^6.0.0",
    "debug": "^4.3.1",
    "end-of-stream": "^1.4.1",
    "fs-extra": "^9.1.0",
    "get-stream": "^6.0.0",
    "lodash": "^4.17.11",
    "media-typer": "^1.1.0",
    "multiparty": "^4.2.1"
  },
  "devDependencies": {
    "@adonisjs/ace": "^5.1.0",
    "@adonisjs/fold": "^4.1.0",
    "@adonisjs/mrm-preset": "^3.0.0",
    "@adonisjs/sink": "^1.1.0",
    "@adonisjs/validator": "^5.1.0",
    "commitizen": "^4.2.3",
    "cz-conventional-changelog": "^3.3.0",
    "japa": "^3.1.1",
    "mrm": "^2.6.1",
    "node-req": "^2.1.1",
    "pkg-ok": "^2.3.1",
    "semver": "^7.3.5",
    "standard": "^16.0.3",
    "supertest": "^6.1.3",
    "yorkie": "^2.0.0",
    "zombie": "^6.1.3"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "nyc": {
    "include": [
      "src"
    ],
    "exclude": [
      "test"
    ]
  }
}
