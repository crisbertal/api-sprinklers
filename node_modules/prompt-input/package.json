{
  "name": "prompt-input",
  "description": "Basic text input prompt. This can be used standalone, but it's also included in [enquirer] by default.",
  "version": "3.0.0",
  "homepage": "https://github.com/enquirer/prompt-input",
  "author": "Jon Schlinkert (https://github.com/jonschlinkert)",
  "contributors": [
    "Brian Woodward (https://twitter.com/doowb)",
    "Jon Schlinkert (http://twitter.com/jonschlinkert)"
  ],
  "repository": "enquirer/prompt-input",
  "bugs": {
    "url": "https://github.com/enquirer/prompt-input/issues"
  },
  "license": "MIT",
  "files": [
    "index.js"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=5.0"
  },
  "scripts": {
    "test": "mocha"
  },
  "dependencies": {
    "debug": "^2.6.8",
    "prompt-base": "^4.0.2"
  },
  "devDependencies": {
    "gulp-format-md": "^1.0.0",
    "mocha": "^3.4.2"
  },
  "keywords": [
    "answer",
    "answers",
    "ask",
    "bar",
    "bottom",
    "checkbox",
    "choice",
    "cli",
    "command",
    "enquirer",
    "input",
    "inquire",
    "inquirer",
    "interact",
    "list",
    "menu",
    "password",
    "prompt",
    "prompts",
    "question",
    "readline",
    "stdin",
    "stdout",
    "terminal",
    "text",
    "tty",
    "ui"
  ],
  "verb": {
    "toc": false,
    "layout": "common-minimal",
    "data": {
      "alias": "input"
    },
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "related": {
      "list": [
        "enquirer",
        "prompt-base",
        "prompt-checkbox",
        "prompt-confirm",
        "prompt-list",
        "prompt-sort"
      ]
    },
    "lint": {
      "reflinks": true
    }
  }
}
